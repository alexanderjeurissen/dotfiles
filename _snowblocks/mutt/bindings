bind index,pager g noop

bind pager gg top
bind index gg first-entry

bind pager G bottom
bind index G last-entry

bind pager j next-line
bind pager k previous-line

bind index za collapse-thread
bind index,pager zj next-subthread
bind index,pager zk previous-subthread

bind alias,index,pager,browser,query \Cd half-down
bind alias,index,pager,browser,query \Cu half-up

macro pager \Co "|urlview<enter>" "call urlview to open links"
