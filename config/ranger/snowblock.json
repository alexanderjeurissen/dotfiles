[
  {
    "clean": [
      "~/.config/ranger"
    ]
  },
  {
    "dependencies": [
       { "name": "ranger-devicons", "cmd": "pip3 install ranger-devicons", "skip": true }
    ]
  },
  {
    "link": {
      "~/.config/ranger/rc.conf": { "create": true, "relink": true }
    },
    "shell": [
      [
        "mkdir -p ~/.config/ranger",
        "Create folder for ranger config files"
      ]
    ]
  }
]
