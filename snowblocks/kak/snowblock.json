[
  {
    "clean": [
      "~/.config/kak",
      "~/.config/kak/rc"
    ]
  },
  {
    "dependencies": [
      { "name": "xcode developer utilities", "cmd": "xcode-select --install" },
      { "name": "kakoune", "cmd": "brew install --HEAD kakoune" }
    ]
  },
  {
    "link": {
      "~/.config/kak/kakrc": { "create": true, "relink": true },
      "~/.config/kak/rc/syntastic.kak": { "create": true, "relink": true }
    }
  }
]
