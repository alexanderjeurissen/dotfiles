[
  {
    "clean": [
      "~",
      "/usr/share/zsh/site-functions",
      "~/.zplug"
    ]
  },
  {
    "dependencies": [
      {
        "name": "zsh",
        "cmd": "brew install zsh"
      },
      {
        "name": "ranger",
        "cmd": "brew install ranger"
      },
      {
        "name": "fzf",
        "cmd": "brew install fzf"
      },
      {
        "name": "terminal-notifier",
        "cmd": "brew install terminal-notifier"
      }
    ]
  },
  {
    "link": {
      "/usr/local/share/zsh/site-functions/completions": {
        "create": true,
        "relink": true
      }
    }
  },
  {
    "link": {
      "~/.zshrc": {
        "relink": true
      },
      "~/.zprofile": {
        "relink": true
      },
      "~/.zshenv": {
        "relink": true
      },
      "~/.zplug": {
        "relink": true
      }
    }
  }
]
