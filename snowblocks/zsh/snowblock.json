[
  {
    "clean": ["~", "/usr/share/zsh/site-functions", "~/.zsh_autoload"]
  },
  {
    "dependencies": [
      {
        "name": "zsh",
        "cmd": "brew install zsh"
      },
      {
        "name": "ranger",
        "cmd": "brew install ranger"
      },
      {
        "name": "fzf",
        "cmd": "brew install fzf"
      },
      {
        "name": "terminal-notifier",
        "cmd": "brew install terminal-notifier"
      }
    ]
  },
  {
    "link": {
      "~/.zshrc": {
        "relink": true
      },
      "~/.zprofile": {
        "relink": true
      },
      "~/.zsh_autoload": {
        "relink": true
      }
    }
  }
]
