" set runtime path for easier loading
" the runtime command lookups the file from
" $HOME/.vim/settings/plugins/Ruby/
set runtimepath+=$HOME/.vim/settings/plugins/Ruby/

runtime bundler.plugin
runtime cucumber.plugin
runtime rails.plugin
runtime rspec.plugin
runtime rspec_snippets.plugin
runtime rubocop.plugin
runtime ruby.plugin
runtime ruby_beautifier.plugin
