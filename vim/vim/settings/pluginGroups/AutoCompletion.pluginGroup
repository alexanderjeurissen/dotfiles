" set runtime path for easier loading
" the runtime command lookups the file from
" $HOME/.vim/settings/plugins/AutoCompletion
set runtimepath+=$HOME/.vim/settings/plugins/AutoCompletion

runtime vimAddonMwUtils.plugin
runtime tlib.plugin
runtime emmet.plugin
runtime repeat.plugin
runtime neocomplete.plugin
runtime ultisnips.plugin
runtime vimReactSnippets.plugin
